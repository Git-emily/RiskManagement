{
    "welcomeFile":"/app/risks/webapp/index.html",
    "authenticationMethod":"route",
    "sessionTimeout":30,
    "logout":{
        "logoutEndpoint":"/do/logout",
        "logoutPage":"/"
    },
    "routes":[
        {
            "source":"^/app/(.*)$",
            "target":"$1",
            "localDir":"resources",
            "authenticationType":"xsuaa"
        },
        {
            "source":"^/app/(.*)$",
            "destination":"srv-binding",
            "authenticationType":"xsuaa" 
        }
    ]
}